{
  "manifest_version": 3,
  "name": "Let's Baseball Game",
  "description": "This extension allows you to play a baseball game",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "128.png"
    }
  },
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "128.png",
    "default_popup": "index.html"
  }
}